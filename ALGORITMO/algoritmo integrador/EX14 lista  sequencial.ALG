Algoritmo "semnome"

Var  dtsa, mtsa, atsa, total: real

Inicio
      escreval("Informe a quantidade de trabalho sem acidentes")
      leia (total)
      mtsa <- total/30
      atsa <- total/360
      dtsa <- total
      escreval("Total dias: ", dtsa, "Total mêses: ", mtsa, "Total anos: ", atsa)


Fimalgoritmo